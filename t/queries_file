SELECT * FROM user WHERE id = 100;
SELECT * FROM user2 WHERE id = 100 LIMIT 3;
SELECT * FROM user2 WHERE point =10.25;
SELECT * FROM user2 WHERE point = +10.25;
SELECT * FROM user2 WHERE point =-10.25;
SELECT * FROM user2 WHERE expression IS TRUE;
SELECT * FROM user2 WHERE expression IS true;
SELECT * FROM user2 WHERE expression IS FALSE;

